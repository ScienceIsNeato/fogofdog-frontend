{
  "expo": {
    "name": "FogOfDog",
    "slug": "fogofdog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fogofdog.app",
      "config": {
        "usesNonExemptEncryption": false
      },
      "buildNumber": "2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fogofdog.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow FogOfDog to use your location."
        }
      ],
      "expo-font"
    ],
    "scheme": "fogofdog",
    "owner": "scienceisneato",
    "extra": {
      "eas": {
        "projectId": "4afe863b-88fa-4153-a38b-cfb7d2d4a71d"
      }
    }
  }
}
