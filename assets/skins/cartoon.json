{
  "version": 8,
  "name": "FogOfDog Cartoon",
  "sources": {
    "openmaptiles": {
      "type": "vector",
      "url": "https://tiles.openfreemap.org/planet"
    }
  },
  "glyphs": "https://demotiles.maplibre.org/font/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#f0e8d0"
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "paint": {
        "fill-color": "#6ec6ff",
        "fill-outline-color": "#2a5d8a"
      }
    },
    {
      "id": "landuse-park",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landuse",
      "filter": ["in", "class", "park", "grass", "cemetery"],
      "paint": {
        "fill-color": "#8dd35f",
        "fill-outline-color": "#5a9e2f"
      }
    },
    {
      "id": "landuse-wood",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landuse",
      "filter": ["in", "class", "wood", "scrub"],
      "paint": {
        "fill-color": "#6abf4b",
        "fill-outline-color": "#3d8f24"
      }
    },
    {
      "id": "landuse-residential",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landuse",
      "filter": ["==", "class", "residential"],
      "paint": {
        "fill-color": "#e8dcc8"
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "building",
      "paint": {
        "fill-color": "#d4a574",
        "fill-outline-color": "#8b6914"
      }
    },
    {
      "id": "road-path",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "path"],
      "paint": {
        "line-color": "#c9a96e",
        "line-width": 2,
        "line-dasharray": [2, 2]
      }
    },
    {
      "id": "road-minor",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "minor", "service"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#ffffff",
        "line-width": {
          "stops": [
            [12, 1],
            [16, 6]
          ]
        }
      }
    },
    {
      "id": "road-minor-outline",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "minor", "service"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#b0a080",
        "line-width": {
          "stops": [
            [12, 2.5],
            [16, 9]
          ]
        }
      }
    },
    {
      "id": "road-secondary",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "secondary", "tertiary"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#fff4c6",
        "line-width": {
          "stops": [
            [10, 2],
            [16, 10]
          ]
        }
      }
    },
    {
      "id": "road-secondary-outline",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "secondary", "tertiary"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#c9a96e",
        "line-width": {
          "stops": [
            [10, 3.5],
            [16, 13]
          ]
        }
      }
    },
    {
      "id": "road-primary",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "primary"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#ffe08a",
        "line-width": {
          "stops": [
            [8, 2],
            [16, 14]
          ]
        }
      }
    },
    {
      "id": "road-primary-outline",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "primary"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#c9a96e",
        "line-width": {
          "stops": [
            [8, 3.5],
            [16, 17]
          ]
        }
      }
    },
    {
      "id": "road-highway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "motorway", "trunk"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#ffb347",
        "line-width": {
          "stops": [
            [6, 2],
            [16, 16]
          ]
        }
      }
    },
    {
      "id": "road-highway-outline",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "motorway", "trunk"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#cc7722",
        "line-width": {
          "stops": [
            [6, 3.5],
            [16, 19]
          ]
        }
      }
    },
    {
      "id": "railway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "rail"],
      "paint": {
        "line-color": "#666666",
        "line-width": 2,
        "line-dasharray": [4, 4]
      }
    },
    {
      "id": "road-label",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "transportation_name",
      "layout": {
        "symbol-placement": "line",
        "text-field": "{name}",
        "text-font": ["Open Sans Regular"],
        "text-size": {
          "stops": [
            [13, 10],
            [16, 14]
          ]
        }
      },
      "paint": {
        "text-color": "#4a3520",
        "text-halo-color": "#f0e8d0",
        "text-halo-width": 2
      }
    },
    {
      "id": "place-label",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Bold"],
        "text-size": {
          "stops": [
            [8, 12],
            [14, 20]
          ]
        }
      },
      "paint": {
        "text-color": "#3d2b1f",
        "text-halo-color": "#f0e8d0",
        "text-halo-width": 2
      }
    }
  ]
}
