{
  "name": "fogofdog",
  "main": "index.ts",
  "version": "1.1.4",
  "license": "UNLICENSED",
  "author": "William Martin <unique.will.martin@gmail.com>",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll=false --passWithNoTests",
    "test:watch": "jest --watchAll",
    "test:ci": "echo 'Starting CI tests...' && node --version && npm --version && jest --version && jest --watchAll=false --passWithNoTests --forceExit --detectOpenHandles --verbose --no-cache",
    "test:e2e": "detox test --configuration ios.sim.debug",
    "test:e2e:release": "detox test --configuration ios.sim.release",
    "build:e2e:ios": "detox build --configuration ios.sim.debug",
    "build:e2e:ios:release": "detox build --configuration ios.sim.release",
    "lint": "expo lint",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:errors-only": "eslint src --ext .ts,.tsx --quiet",
    "lint:strict": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:transition": "eslint src --ext .ts,.tsx --max-warnings 35",
    "duplication:check": "npx jscpd src --min-lines 3 --threshold 3 --reporters console --ignore '**/__tests__/**,**/*.test.*'",
    "duplication:report": "npx jscpd src --min-lines 3 --threshold 3 --reporters html,console --output ./reports",
    "duplication:strict": "npx jscpd src --min-lines 3 --threshold 2 --reporters console",
    "duplication:ci": "npx jscpd src --min-lines 3 --threshold 3 --reporters console --silent",
    "pre-commit": "sm validate --quality-gates javascript:lint-format",
    "validate": "sm validate javascript",
    "validate:pr": "sm validate javascript && sm validate --quality-gates quality:duplication",
    "validate:quick": "sm validate --quality-gates javascript:lint-format",
    "format:auto-fix": "npm run format:fix && npm run format:check",
    "lint:auto-fix": "npm run lint:fix && npm run lint:strict",
    "audit:auto-fix": "npm audit fix && npm run audit:security",
    "prepare": "husky",
    "quality:check": "sm validate javascript && sm validate --quality-gates quality:duplication",
    "quality:full": "npm run quality:check && npm run type-check",
    "quality:advanced": "npm run format:check && npm run deadcode:check && npm run deps:check",
    "quality:fix": "npm run lint:fix && npm run format:fix && npm run test:update-snapshots",
    "format:check": "npx prettier --check src/",
    "format:fix": "npx prettier --write src/",
    "imports:check": "echo 'Import checking integrated with main linting'",
    "deadcode:check": "npx unimported",
    "deps:vulnerabilities": "npx audit-ci --config .audit-ci.json",
    "deps:outdated": "npx npm-check-updates --target minor",
    "complexity:check": "echo 'Complexity rules integrated with main linting'",
    "audit:security": "npm audit --audit-level high",
    "type-check": "npx tsc --noEmit --strict",
    "type-check-fast": "npx tsc --noEmit",
    "type-check-ci": "npx tsc --noEmit --project tsconfig.ci.json",
    "type-check-verbose": "npx tsc --noEmit --strict --pretty --listFiles",
    "type-check-watch": "npx tsc --noEmit --strict --watch",
    "type-check-debug": "npx tsc --noEmit --strict --pretty --listEmittedFiles --listFiles --traceResolution",
    "test:coverage": "jest --coverage --watchAll=false --passWithNoTests",
    "test:update-snapshots": "jest --updateSnapshot --watchAll=false",
    "bundle:analyze": "npx expo export --platform ios && echo 'Bundle analysis complete'",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && npm install",
    "ci:quality-gate": "sm validate javascript && sm validate --quality-gates quality:duplication",
    "quality:complexity": "npx complexity-report --format json --output complexity-report.json src/ --exclude '**/node_modules/**' --exclude '**/__mocks__/**'",
    "quality:duplicates": "npx jscpd src/ --reporters console,json --output ./quality-reports/ --ignore '**/__mocks__/**,**/*.test.*,**/__tests__/**'",
    "quality:dependencies": "npx madge --circular --extensions ts,tsx --exclude '__mocks__|__tests__|test' src/",
    "quality:full-analysis": "npm run quality:complexity && npm run quality:duplicates && npm run quality:dependencies",
    "quality:report": "npm run test:coverage && npm run quality:full-analysis",
    "build:verify": "npx expo export --platform ios && npx expo export --platform android",
    "version:major": "node scripts/version-bump.js major",
    "version:minor": "node scripts/version-bump.js minor",
    "version:patch": "node scripts/version-bump.js patch",
    "version:check": "node -p \"require('./package.json').version\""
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@maplibre/maplibre-react-native": "^10.4.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.1.10",
    "@react-navigation/native-stack": "^7.3.14",
    "@reduxjs/toolkit": "^2.8.2",
    "@shopify/react-native-skia": "^2.4.18",
    "expo": "^54.0.33",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "18.0.12",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-task-manager": "~14.0.9",
    "expo-updates": "~29.0.16",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "^1.27.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.5.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@eslint/eslintrc": "^3.3.1",
    "@expo/ngrok": "^4.1.3",
    "@react-native-community/cli": "^18.0.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "audit-ci": "^7.1.0",
    "babel-preset-expo": "~54.0.10",
    "complexity-report": "^2.0.0-alpha",
    "detox": "^20.39.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-sonarjs": "^3.0.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jscpd": "^4.0.5",
    "madge": "^8.0.0",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.5.3",
    "react-test-renderer": "^19.1.0",
    "sonarqube-scanner": "^4.3.0",
    "typescript": "~5.9.2",
    "unimported": "^1.31.0"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      },
      "checkPackages": false
    }
  }
}
