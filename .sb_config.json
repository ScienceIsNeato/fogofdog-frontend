      "template-syntax": {
        "enabled": true,
        "auto_fix": false,
        "config_file_path": null,
        "templates_dir": null
      }
    }
  },
  "myopia": {
    "enabled": true,
    "include_dirs": [],
    "exclude_dirs": ["slop-mop"],
    "gates": {
      "security-audit": {
        "enabled": true,
        "auto_fix": false,
        "config_file_path": ".secrets.baseline",
        "scanners": ["bandit", "semgrep", "detect-secrets"],
        "exclude_dirs": [
          "venv",
          ".venv",
          "node_modules",
          "cursor-rules",
          "archives",
          "logs",
          "tests",
          "*/.venv",
          "*/venv"
        ],
        "bandit_config_file": null
      },
      "security-scan": {
        "enabled": true,
        "auto_fix": false,
        "config_file_path": ".secrets.baseline",
        "scanners": ["bandit", "semgrep", "detect-secrets"],
        "exclude_dirs": [
          "venv",
          ".venv",
          "node_modules",
          "cursor-rules",
          "archives",
          "logs",
          "tests",
          "*/.venv",
          "*/venv"
