        "bandit_config_file": null
      },
      "source-duplication": {
        "enabled": true,
        "auto_fix": false,
        "config_file_path": null,
        "threshold": 5,
        "include_dirs": ["src"],
        "min_tokens": 50,
        "min_lines": 5,
        "exclude_dirs": []
      },
      "string-duplication": {
        "enabled": true,
        "auto_fix": false,
        "config_file_path": null,
        "threshold": 2,
        "min_file_count": 1,
        "min_length": 8,
        "min_words": 3,
        "include_patterns": ["**/*.py"],
        "ignore_patterns": [
          "**/node_modules/**",
          "**/.venv/**",
          "**/venv/**",
          "**/__pycache__/**",
          "**/.git/**",
          "**/*.egg-info/**",
          "**/test_*",
          "**/tests/**",
          "**/conftest.py"
        ]
      },
      "loc-lock": {
        "enabled": true,
        "auto_fix": false,
        "config_file_path": null,
        "max_file_lines": 1000,
        "max_function_lines": 120,
        "include_dirs": ["src", "scripts"],
        "exclude_dirs": [
          "ios",
          "android",
          "node_modules",
          "coverage",
          ".expo",
          "temp",
          "test_artifacts",
          "quality-reports",
          "reports",
          "slop-mop",
          "cursor-rules",
          "__mocks__",
          "__tests__"
        ],
        "extensions": []
