        "enabled": true,
        "auto_fix": false,
        "config_file_path": null,
        "test_dirs": ["tests"],
        "exclude_patterns": ["conftest.py"]
      }
    }
  },
  "laziness": {
    "enabled": true,
    "include_dirs": [],
    "exclude_dirs": ["slop-mop"],
    "gates": {
      "py-lint": {
        "enabled": true,
        "auto_fix": false,
        "config_file_path": null,
