        "auto_fix": false,
        "config_file_path": null,
        "test_script": "scripts/__tests__/deploy_app.test.sh"
      }
    }
  },
  "deceptiveness": {
    "enabled": true,
    "include_dirs": [],
    "exclude_dirs": ["slop-mop"],
