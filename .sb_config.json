      "complexity": {
        "enabled": true,
        "auto_fix": false,
        "config_file_path": null,
        "max_rank": "C",
        "max_complexity": 15,
        "src_dirs": []
      },
      "dead-code": {
        "enabled": true,
        "auto_fix": false,
        "config_file_path": null,
        "min_confidence": 80,
        "exclude_patterns": [
          "**/venv/**",
          "**/.venv/**",
          "**/node_modules/**",
          "**/test_*",
          "**/tests/**",
          "**/conftest.py",
          "**/*.egg-info/**",
          "**/build/**",
          "**/dist/**",
          "**/cursor-rules/**"
