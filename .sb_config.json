{
  "version": "1.0",
  "default_profile": "pr",
  "python": {
    "enabled": true,
    "include_dirs": ["scripts"],
    "exclude_dirs": [],
    "gates": {
      "lint-format": {
        "enabled": false,
        "auto_fix": false,
        "config_file_path": null,
        "line_length": 88
      },
      "tests": {
        "enabled": false,
        "auto_fix": false,
        "config_file_path": null,
        "test_dirs": ["tests"],
        "timeout": 300
      },
      "coverage": {
        "enabled": false,
        "auto_fix": false,
        "config_file_path": null,
        "threshold": 80
      },
      "diff-coverage": {
        "enabled": false,
        "auto_fix": false,
        "config_file_path": null
      },
      "new-code-coverage": {
        "enabled": false,
        "auto_fix": false,
        "config_file_path": null
      },
      "static-analysis": {
        "enabled": false,
        "auto_fix": false,
        "config_file_path": null
      }
    }
  },
  "javascript": {
    "enabled": true,
    "include_dirs": [],
    "exclude_dirs": [],
    "gates": {
      "lint-format": {
        "enabled": true,
        "auto_fix": true,
        "config_file_path": null
      },
      "tests": {
        "enabled": true,
        "auto_fix": false,
        "config_file_path": null,
        "test_command": "npx jest --passWithNoTests --forceExit --no-coverage"
      },
      "coverage": {
        "enabled": true,
        "auto_fix": false,
        "config_file_path": null,
        "threshold": 78
      },
      "frontend": {
        "enabled": false,
        "auto_fix": false,
        "config_file_path": null,
        "frontend_dirs": []
      }
    }
  },
  "security": {
    "enabled": false,
    "include_dirs": [],
    "exclude_dirs": [],
    "gates": {
      "full": {
        "enabled": false,
        "auto_fix": false,
        "config_file_path": ".secrets.baseline",
        "scanners": ["bandit", "semgrep", "detect-secrets"],
        "exclude_dirs": [
          "venv",
          ".venv",
          "node_modules",
          "cursor-rules",
          "archives",
          "logs",
          "tests",
          "*/.venv",
          "*/venv"
        ]
      },
      "local": {
        "enabled": false,
        "auto_fix": false,
        "config_file_path": ".secrets.baseline",
        "scanners": ["bandit", "semgrep", "detect-secrets"],
        "exclude_dirs": [
          "venv",
          ".venv",
          "node_modules",
          "cursor-rules",
          "archives",
          "logs",
          "tests",
          "*/.venv",
          "*/venv"
        ]
      }
    }
  },
  "quality": {
    "enabled": true,
    "include_dirs": [],
    "exclude_dirs": ["coverage", "ios", "android", "node_modules", ".git"],
    "gates": {
      "complexity": {
        "enabled": false,
        "auto_fix": false,
        "config_file_path": null,
        "max_rank": "C",
        "max_complexity": 15,
        "src_dirs": []
      },
      "duplication": {
        "enabled": true,
        "auto_fix": false,
        "config_file_path": null,
        "threshold": 5,
        "include_dirs": ["src"],
        "min_tokens": 50,
        "min_lines": 5
      }
    }
  },
  "general": {
    "enabled": false,
    "include_dirs": [],
    "exclude_dirs": [],
    "gates": {
      "templates": {
        "enabled": false,
        "auto_fix": false,
        "config_file_path": null,
        "templates_dir": null
      }
    }
  },
  "integration": {
    "enabled": false,
    "include_dirs": [],
    "exclude_dirs": [],
    "gates": {
      "smoke-tests": {
        "enabled": false,
        "auto_fix": false,
        "config_file_path": null,
        "test_dir": "tests/smoke"
      },
      "integration-tests": {
        "enabled": false,
        "auto_fix": false,
        "config_file_path": null,
        "test_dir": "tests/integration"
      },
      "e2e-tests": {
        "enabled": false,
        "auto_fix": false,
        "config_file_path": null,
        "test_dir": "tests/e2e",
        "test_command": null
      }
    }
  },
  "pr": {
    "enabled": true,
    "include_dirs": [],
    "exclude_dirs": [],
    "gates": {
      "comments": {
        "enabled": true,
        "auto_fix": false,
        "config_file_path": null,
        "fail_on_unresolved": true
      }
    }
  }
}
