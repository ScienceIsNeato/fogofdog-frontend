{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "device": {
      "distribution": "internal",
      "ios": {
        "image": "latest"
      }
    },
    "testflight": {
      "distribution": "store",
      "ios": {
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "distribution": "store",
      "ios": {
        "image": "latest"
      },
      "autoIncrement": true
    }
  }
} 